<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="T3ME.Domain" namespace="T3ME.Domain.Models">

    <class name="Language" table="Languages">
        <id name="Id" type="Int64" unsaved-value="0">
            <generator class="identity" />
        </id>

        <property name="Code" not-null="true" type="String" length="50" unique="true" />
        <property name="Name" not-null="false" type="String" length="50" />
        <property name="IsRecognised" not-null="true" type="Boolean" />
        
    </class>
    
</hibernate-mapping>